{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all",
    "local>templates/renovate-config",
    "local>templates/renovate-config:security"
  ],
  "assignees": [
    "gjrtimmer"
  ],
  "packageRules": [
    {
      "description": "Bypass Image Versions",
      "enabled": false,
      "matchFileNames": [
        "Dockerfile.3*"
      ],
      "matchDatasources": [
        "docker"
      ]
    }
  ]
}